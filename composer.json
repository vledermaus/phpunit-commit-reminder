{
    "name": "vledermaus/phpunit-commit-reminder",
    "description": "phpunit-commit-reminder: A PHPUnit extension that reminds developers to commit code changes after successful tests, promoting code stability.",
    "keywords": [
        "phpunit",
        "commit",
        "reminder",
        "extension"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vledermaus\\PHPUnitCommitReminder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vledermaus\\PHPUnitCommitReminder\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Daniel Ravenshorst",
            "email": "d.ravenshorst@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "friendsofphp/php-cs-fixer": "^3.34"
    },
    "require": {
        "czproject/git-php": "^4.2"
    }
}
